{
  "name": "ts-auto-mock",
  "version": "1.0.0",
  "description": "Typescript transformer to unlock automatic mock creation for interfaces and classes",
  "main": "dist/index.js",
  "scripts": {
    "build:transformer": "webpack --config config/modules/transformer/webpack.js",
    "build:mock": "webpack --config config/modules/mock/webpack.js",
    "build:repository": "webpack --config config/modules/repository/webpack.js",
    "build:extension": "webpack --config config/modules/extension/webpack.js",
    "build": "npm run build:mock && npm run build:transformer && npm run build:repository && npm run build:extension",
    "test": "npm run test:transformer && npm run test:framework",
    "test:transformer": "karma start config/karma/karma.config.transformer.js",
    "test:framework": "karma start config/karma/karma.config.framework.js",
    "prepublishOnly": "npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "keywords": [
    "typescript",
    "spy",
    "mock",
    "auto"
  ],
  "files": [
    "dist/**/*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uittorio/ts-auto-mock.git"
  },
  "author": "Vittorio Guerriero & Giulio Caprino",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-angular": "7.5.0",
    "@types/jasmine": "3.3.5",
    "@types/node": "10.12.18",
    "@types/webpack-env": "1.13.6",
    "awesome-typescript-loader": "5.2.1",
    "clean-webpack-plugin": "2.0.1",
    "conventional-changelog-cli": "2.0.12",
    "copy-webpack-plugin": "5.0.2",
    "git-branch-is": "2.1.0",
    "husky": "1.3.1",
    "jasmine": "3.3.1",
    "karma": "3.1.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "2.0.1",
    "karma-webpack": "3.0.5",
    "puppeteer": "1.14.0",
    "ts-loader": "5.3.2",
    "ts-node": "7.0.1",
    "tslint": "5.15.0",
    "tslint-loader": "3.5.4",
    "typescript": "3.2.2",
    "webpack": "4.28.3",
    "webpack-cli": "3.1.2",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "url-slug": "2.0.0",
    "path": "0.12.7"
  },
  "peerDependencies": {
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "if git-branch-is master; then commitlint -E HUSKY_GIT_PARAMS; fi"
    }
  }
}
