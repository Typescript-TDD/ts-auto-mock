{
  "name": "ts-auto-mock",
  "version": "1.1.4",
  "description": "Typescript transformer to unlock automatic mock creation for interfaces and classes",
  "scripts": {
    "build:transformer": "webpack --config config/modules/transformer/webpack.js",
    "build:mock": "webpack --config config/modules/mock/webpack.js",
    "build:repository": "webpack --config config/modules/repository/webpack.js",
    "build:extension": "webpack --config config/modules/extension/webpack.js",
    "build": "npm run build:mock && npm run build:transformer && npm run build:repository && npm run build:extension",
    "test": "npm run test:transformer && npm run test:framework",
    "test:debug": "npm run test:transformer DEBUG=true && npm run test:framework DEBUG=true",
    "test:transformer": "karma start config/karma/karma.config.transformer.js",
    "test:framework": "karma start config/karma/karma.config.framework.js",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "copyForPublish": "cp -rf package.json README.md CHANGELOG.md dist",
    "preparePublish": "npm run build && npm run copyForPublish"
  },
  "keywords": [
    "typescript",
    "spy",
    "mock",
    "auto"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uittorio/ts-auto-mock.git"
  },
  "author": "Vittorio Guerriero & Giulio Caprino",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@types/jasmine": "^3.3.16",
    "@types/node": "^12.6.9",
    "@types/webpack-env": "^1.14.0",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "conventional-changelog-cli": "^2.0.23",
    "copy-webpack-plugin": "^5.0.4",
    "git-branch-is": "^2.1.0",
    "husky": "^3.0.2",
    "jasmine": "^3.4.0",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-webpack": "^4.0.2",
    "puppeteer": "^1.19.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "url-slug": "^2.1.3",
    "winston": "^3.2.1"
  },
  "peerDependencies": {
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "if git-branch-is master; then commitlint -E HUSKY_GIT_PARAMS; fi"
    }
  }
}
